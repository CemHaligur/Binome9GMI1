package com.company;

import java.util.Scanner;

public class Main {
    public Main() {
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Saisir 2 entiers : ");
        int value1 = sc.nextInt();
        int value2 = sc.nextInt();
        System.out.print(value1 + value2);
        sc.close();
    }
}
